{
  "name": "fast-form-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "heroku-postbuild": "npm run build && npm run typeorm:migration:run",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli.config.ts",
    "typeorm:migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli.config.ts migration:create -n",
    "typeorm:migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli.config.ts migration:generate -n",
    "typeorm:migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli.config.ts migration:revert",
    "typeorm:migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli.config.ts migration:run",
    "typeorm:seed:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli-seed.config.ts migration:create -n",
    "typeorm:seed:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli-seed.config.ts migration:generate -n",
    "typeorm:seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli-seed.config.ts migration:run",
    "typeorm:seed:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/database-cli-seed.config.ts migration:revert",
    "prepare": "husky install"
  },
  "dependencies": {
    "@casl/ability": "^5.2.2",
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/node": "^6.5.1",
    "@sentry/tracing": "^6.5.1",
    "bcryptjs": "^2.4.3",
    "brazilian-class-validator": "^0.1.6",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "date-fns": "^2.22.1",
    "express-basic-auth": "^1.2.0",
    "express-rate-limit": "^5.2.6",
    "faker": "^5.5.3",
    "firebase-admin": "^9.7.0",
    "helmet": "^4.4.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.6.2",
    "objects-to-csv": "^1.3.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-transactional-cls-hooked": "^0.1.20"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.170",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.31",
    "@types/nodemailer": "^6.4.2",
    "@types/objects-to-csv": "^1.3.0",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
